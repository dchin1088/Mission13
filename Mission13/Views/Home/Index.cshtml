@{
    ViewData["Title"] = "Home Page";
}

@model List<Bowler>

<h1>Bowlers</h1>

<ul>
    @foreach (var r in Model)
    {
    <li>@r.BowlerID - @r.BowlerLastName - @r.BowlerFirstName - @r.BowlerMiddleInit . - @r.BowlerAddress - @r.BowlerCity - @r.BowlerState - @r.BowlerZip - @r.BowlerPhoneNumber - @r.TeamID</li>
    }
</ul>
